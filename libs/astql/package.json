{
  "name": "astql",
  "version": "0.0.6",
  "author": "Vasilis Kefallinos <vkefallinos@gmail.com>",
  "contributors": [],
  "description": "A query library for ECMAScript AST using a CSS selector like query language.",
  "main": "dist/index.js",
  "files": [
    "license.txt",
    "README.md"
  ],
  "nyc": {
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "reporter": [
      "html",
      "text"
    ],
    "exclude": [
      "parser.js",
      "dist",
      "tests"
    ]
  },
  "scripts": {
    "build": "digigov build --subpackages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gencodes/gencodes.git"
  },
  "bugs": "https://github.com/gencodes/gencodes/issues",
  "homepage": "https://github.com/gencodes/gencodes/",
  "keywords": [
    "ast",
    "ecmascript",
    "javascript",
    "query"
  ],
  "devDependencies": {
    "pegjs": "~0.10.0",
    "typescript": "^4.0.3",
    "webpack":"~5.0.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.1.14",
    "ts-loader":"~9.0.0"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=0.10"
  },
  "dependencies": {
    "glob":"~7.2.0",
    "@digigov/cli":"~0.5.24",
    "@digigov/cli-build":"0.5.24",
    "rimraf":"~3.0.2"
  }
}
