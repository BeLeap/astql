  "dependencies": {
    "peggy": "~1.2.0"
  },
  "devDependencies": {
    "eslint": "~7.24.0",
    "@types/jest": "27.0.2",
    "@types/node": "~16.11.6",
    "@digigov/cli": "~0.5.24",
    "@digigov/cli-build": "0.5.24",
    "rimraf": "~3.0.2",
    "@digigov/cli-test": "0.5.24",
    "typescript": "4.4.4",
    "@digigov/cli-lint": "~0.5.25"
